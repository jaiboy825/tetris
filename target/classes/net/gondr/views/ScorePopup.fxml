<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="130.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.gondr.views.ScorePopupController">
   <children>
      <TextField fx:id="txtName" layoutX="14.0" layoutY="66.0" prefHeight="38.0" prefWidth="320.0" promptText="기록될 이름을 입력하세요">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button layoutX="341.0" layoutY="66.0" mnemonicParsing="false" onAction="#record" prefHeight="38.0" prefWidth="52.0" text="기록" />
      <Label fx:id="lblScore" layoutX="14.0" layoutY="14.0" prefHeight="45.0" prefWidth="377.0" text="당신의 점수는 : 100점">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label fx:id="lblError" layoutX="14.0" layoutY="109.0" prefHeight="14.0" prefWidth="320.0" text="error label" textFill="RED">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
   </children>
</AnchorPane>